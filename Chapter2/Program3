/ Program3.cpp : This program converts gallons to liters
//

#include "stdafx.h"
#include <iostream>
using namespace std;

int main()

{
	int gallons, liters;

	cout << "Enter number of gallons: ";
	cin >> gallons; //This inputs from the user

	liters = gallons * 4; // Gallons to liters conversion
	
	cout << "Liters: " << liters;

	return 0;
}

